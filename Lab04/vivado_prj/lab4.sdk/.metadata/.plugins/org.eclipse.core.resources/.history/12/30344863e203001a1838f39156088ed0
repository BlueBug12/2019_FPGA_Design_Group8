

/***************************** Include Files *******************************/
#include "calculator.h"

/************************** Function Definitions ***************************/
int calculator(u32 baseAddr, u32 op, int a, int b){
	CALCULATOR_mWriteReg(baseAddr, 0, op);
	CALCULATOR_mWriteReg(baseAddr, 4, a);
	CALCULATOR_mWriteReg(baseAddr, 8, b);



}
